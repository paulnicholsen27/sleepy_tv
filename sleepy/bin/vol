#!/Users/paulnicholsen/Documents/code/sleepy_tv/sleepy/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'vol==0.1.1','console_scripts','vol'
__requires__ = 'vol==0.1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vol==0.1.1', 'console_scripts', 'vol')()
    )
